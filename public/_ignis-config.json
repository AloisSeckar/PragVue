{
  "modules": [
    "@nuxt/eslint",
    "@nuxt/fonts",
    "@nuxt/image",
    "@nuxt/scripts",
    "nuxt-security",
    "@vueuse/nuxt",
    "@pinia/nuxt",
    "@nuxtjs/i18n"
  ],
  "vue": {
    "compilerOptions": {},
    "transformAssetUrls": {
      "video": [
        "src",
        "poster"
      ],
      "source": [
        "src"
      ],
      "img": [
        "src"
      ],
      "image": [
        "xlink:href",
        "href"
      ],
      "use": [
        "xlink:href",
        "href"
      ]
    },
    "runtimeCompiler": false,
    "propsDestructure": true,
    "config": {}
  },
  "css": [
    "C:\\Git\\PragVue\\node_modules\\.pnpm\\nuxt-ignis@0.4.0-rc.1_779928bc48e0b18bbbfc124a91b5d433\\node_modules\\nuxt-ignis\\assets\\css\\ignis-tailwind.css"
  ],
  "vite": {
    "plugins": [
      {
        "name": "vite-plugin-ignore-sourcemap-warnings",
        "apply": "build"
      },
      [
        {
          "name": "@tailwindcss/vite:scan",
          "enforce": "pre"
        },
        {
          "name": "@tailwindcss/vite:generate:serve",
          "apply": "serve",
          "enforce": "pre"
        },
        {
          "name": "@tailwindcss/vite:generate:build",
          "apply": "build",
          "enforce": "pre"
        }
      ]
    ]
  },
  "i18n": {
    "vueI18n": "./i18n/i18n.config.ts",
    "locales": [
      "en"
    ],
    "strategy": "no_prefix",
    "bundle": {
      "optimizeTranslationDirective": false
    }
  },
  "app": {
    "head": {
      "meta": [
        {
          "name": "viewport",
          "content": "width=device-width, initial-scale=1"
        },
        {
          "charset": "utf-8"
        }
      ],
      "link": [],
      "style": [],
      "script": [],
      "noscript": [],
      "htmlAttrs": {
        "lang": "en"
      },
      "title": "Nuxt Ignis App"
    },
    "baseURL": "/",
    "buildAssetsDir": "/_nuxt/",
    "cdnURL": "",
    "layoutTransition": false,
    "pageTransition": false,
    "viewTransition": false,
    "keepalive": false,
    "rootId": "__nuxt",
    "rootTag": "div",
    "rootAttrs": {
      "id": "__nuxt"
    },
    "teleportTag": "div",
    "teleportId": "teleports",
    "teleportAttrs": {
      "id": "teleports"
    },
    "spaLoaderTag": "div",
    "spaLoaderAttrs": {
      "id": "__nuxt-loader"
    }
  }
}